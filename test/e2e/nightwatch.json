{
    "src_folders": [
        "./test/e2e/"
    ],
    "output_folder": "./output",
    "custom_commands_path": null,
    "custom_assertions_path": null,
    "page_objects_path": null,
    "globals_path": "./globals.js",
    "disable_colors": false,
    "test_workers": false,
    "webdriver": {
        "start_process": false,
        "host": "localhost",
        "port": 8080
    },
    "launch_url": "http://localhost:8081?stargate=http://localhost:8080&&rpc=http://localhost:26657",
    "test_settings": {
        "default": {
            "silent": true,
            "webdriver": {
                "start_process": true,
                "port": 9515,
                "default_path_prefix": "",
                "server_path": "./node_modules/.bin/chromedriver",
                "cli_args": [
                    "--verbose"
                ]
            },
            "desiredCapabilities": {
                "browserName": "chrome",
                "javascriptEnabled": true,
                "acceptSslCerts": true,
                "loggingPrefs": {
                    "driver": "INFO",
                    "server": "OFF",
                    "browser": "INFO"
                },
                "chromeOptions": {
                    "args": [
                        "headless",
                        "disable-gpu=true",
                        "disable-web-security",
                        "ignore-certificate-errors"
                    ]
                }
            },
            "disable_colors": false,
            "screenshots": {
                "enabled": false,
                "path": ""
            },
            "request_timeout_options": {
                "timeout": 60000,
                "retry_attempts": 5
            },
            "filter": [
                "*.spec.js"
            ]
        }
    }
}